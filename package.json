{
  "name": "react-typescript-music-event-list",
  "version": "0.1.0",
  "description": "Music event list built with React, TypeScript and styled components",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testMatch=\"**/*.(test).{ts,tsx}\"",
    "coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "npm run lint --fix",
    "stylelint": "stylelint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --config .prettierrc --ignore-path .gitignore \"src/**/*.{js,ts,tsx,json}\" --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elenabravo/dice-frontend-task"
  },
  "author": "Elena Bravo<hey@elenabravo.com>",
  "dependencies": {
    "@axe-core/react": "^4.4.3",
    "@chakra-ui/react": "^2.3.2",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.7",
    "cross-fetch": "^3.1.5",
    "framer-motion": "^7.3.2",
    "msw": "^0.44.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "use-debounce": "^8.0.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "axe-core": "^4.4.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-axe": "^6.0.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0"
  }
